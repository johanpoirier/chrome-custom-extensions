{
  "manifest_version": 3,
  "name": "Github PR check all files",
  "description": "Github PR check all files",
  "version": "1.1",

  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://github.com/dktunited/cloakroom/pull/*/files",
        "https://github.com/dktunited/myaccount-bff/pull/*/files"
      ]
    }
  ]
}
